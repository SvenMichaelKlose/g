all:
	$(MAKE) -C lib/gfx all
	$(MAKE) -C lib/gui all
	$(MAKE) -C lib/ultimem-basics all
	$(MAKE) -C flashboot all
	$(MAKE) -C flashmenu all
	$(MAKE) -C paint all

clean:
	$(MAKE) -C lib/gfx clean
	$(MAKE) -C lib/gui clean
	$(MAKE) -C lib/ultimem-basics clean
	$(MAKE) -C flashboot clean
	$(MAKE) -C flashmenu clean
	$(MAKE) -C paint clean
